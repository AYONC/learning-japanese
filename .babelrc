{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      }
    ],
    ["@babel/preset-es2017"],
    ["@babel/preset-react",
      {
        "throwIfNamespace": false
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-proposal-object-rest-spread"],
    ["@babel/plugin-transform-react-jsx"],
    ["module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "main": "./src/main",
          "renderer": "./src/renderer"
        }
      }
    ]
  ]
}

